		<value condition="Player.Paused + ![Player.Caching | Player.Seeking]">[COLOR=white][B]$LOCALIZE[31302][/B][/COLOR]</value>
		<value condition="!String.IsEmpty(Player.SeekStepSize) + ![Player.Forwarding | Player.Rewinding]">[COLOR=white][B]$INFO[Player.SeekStepSize][/B][/COLOR]</value>
		<value condition="Player.Seeking + !String.IsEmpty(Player.SeekOffset)">[COLOR=white][B]$INFO[Player.SeekOffset][/B][/COLOR]</value>
		<value condition="Player.Rewinding2x + !Player.Seeking">[COLOR=white][B]x2[/B][/COLOR]</value>
		<value condition="Player.Rewinding4x + !Player.Seeking">[COLOR=white][B]x4[/B][/COLOR]</value>
		<value condition="Player.Rewinding8x + !Player.Seeking">[COLOR=white][B]x8[/B][/COLOR]</value>
		<value condition="Player.Rewinding16x + !Player.Seeking">[COLOR=white][B]x16[/B][/COLOR]</value>
		<value condition="Player.Rewinding32x + !Player.Seeking">[COLOR=white][B]x32[/B][/COLOR]</value>
		<value condition="Player.Forwarding2x + !Player.Seeking">[COLOR=white][B]x2[/B][/COLOR]</value>
		<value condition="Player.Forwarding4x + !Player.Seeking">[COLOR=white][B]x4[/B][/COLOR]</value>
		<value condition="Player.Forwarding8x + !Player.Seeking">[COLOR=white][B]x8[/B][/COLOR]</value>
		<value condition="Player.Forwarding16x + !Player.Seeking">[COLOR=white][B]x16[/B][/COLOR]</value>
		<value condition="Player.Forwarding32x + !Player.Seeking">[COLOR=white][B]x32[/B][/COLOR]</value>
		<value condition="Player.DisplayAfterSeek + !Player.Seeking + !String.IsEmpty(Player.SeekOffset)">[COLOR=white][B]$INFO[Player.SeekOffset][/B][/COLOR]</value>
	</variable>


	<variable name="DialogSeekBarIcons">
		<value condition="Player.Forwarding">osd/osd_ff.png</value>
		<value condition="Player.Rewinding">osd/osd_rw.png</value>
		<value condition="Player.Paused | Player.Caching">osd/osd_pause.png</value>
	</variable>

	<variable name="PlayerArtPoster">
		<value condition="!String.IsEmpty(Player.Art(poster))">$INFO[Player.Art(poster)]</value>
		<value condition="VideoPlayer.Content(Episodes) + !String.IsEmpty(Player.Art(tvshow.poster))">$INFO[Player.Art(tvshow.poster)]</value>
	</variable>

	<variable name="Vis-Background">
		<value condition="Skin.HasSetting(Vis-Fanart) + !String.IsEmpty(Player.Art(fanart))">$INFO[Player.Art(fanart)]</value>
		<value condition="Skin.HasSetting(Vis-ArtistSlideshow) + !String.IsEmpty(Window(Visualisation).Property(ArtistSlideshow.Image))">$INFO[Window(Visualisation).Property(ArtistSlideshow.Image)]</value>
		<value condition="Skin.HasSetting(Vis-MusicSlideshow) + !String.IsEmpty(Window(home).Property(EmbuaryBackgroundMusic))">$INFO[Window(home).Property(EmbuaryBackgroundMusic)]</value>
		<value>empty_icon.png</value>
	</variable>

</includes>
