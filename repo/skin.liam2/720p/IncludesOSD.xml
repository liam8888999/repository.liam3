	<include name="PlayerOverlayState">
		<control type="image" id="1">
			<width>165</width>
			<height>$PARAM[bg_height]</height>
			<aspectratio scalediffuse="false">scale</aspectratio>
			<texture background="true" border="1" diffuse="$PARAM[bg_diffuse]">thumbs/thumb_back.png</texture>
			<visible>$PARAM[visible]</visible>
			<animation type="conditional" condition="$PARAM[visible]" reversible="false">
				<effect type="fade" start="0" end="85" time="250"/>
			</animation>
			<animation effect="fade" start="80" end="0" time="350">Hidden</animation>
		</control>
		<control type="image" id="1">
			<left>$PARAM[icon_left]</left>
			<top>$PARAM[icon_top]</top>
			<width>$PARAM[icon_width]</width>
			<height>$PARAM[icon_height]</height>
			<texture background="true" border="1">$PARAM[icon]</texture>
			<visible>$PARAM[visible]</visible>
			<animation type="conditional" condition="$PARAM[visible]" reversible="false">
				<effect type="fade" start="0" end="100" time="250"/>
			</animation>
			<animation effect="fade" start="80" end="0" time="350">Hidden</animation>
		</control>
	</include>

