		<include name="MusicOSDStyleDefault">
		<!-- Player Controls -->
		<control type="group">
			<include>OSDAnimation</include>
			<visible>!Window.IsActive(AddonSettings) + !Window.IsActive(SelectDialog) + !Window.IsActive(busydialog) + !Window.IsActive(VisualisationPresetList) + !Window.IsActive(script-cu-lrclyrics-main.xml) + !Window.IsActive(1112) + !Window.IsActive(ShutdownMenu) + !Window.IsActive(favourites)</visible>
			<control type="grouplist" id="100">
				<right>40</right>
				<top>585</top>
				<width>505</width>
				<height>58</height>
				<itemgap>-8</itemgap>
				<onleft>100</onleft>
				<onright>100</onright>
				<onup>1000</onup>
				<ondown>1000</ondown>
				<align>right</align>
				<orientation>horizontal</orientation>
				<control type="button" id="115">
					<include>OSDControlsButton</include>
					<texturefocus colordiffuse="$VAR[Textures.GlobalColor]">osd/controls/simple_setting_ver_fo.png</texturefocus>
					<texturenofocus>osd/controls/simple_setting_ver_nf.png</texturenofocus>
					<onclick>Action(back)</onclick>
					<onclick>Skin.SetString(FullScreenMusicStyle,2)</onclick>
					<onclick>ActivateWindow(1110)</onclick>
